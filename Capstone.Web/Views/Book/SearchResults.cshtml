@model Capstone.Web.Models.SearchResultModel

@{
    ViewBag.Title = "SearchResults";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


    @foreach (var item in Model.Results)
    {<div class="blockForList">


            <img src="@item.ImageLink" alt="Picture of Cover" />

        <div id="searchResultListItem">
          

                @Html.ActionLink(@item.Title, "BookDetail", "Book", new { bookID = item.BookID }, null)
                @item.Author


         

        </div>
    </div>
    }



