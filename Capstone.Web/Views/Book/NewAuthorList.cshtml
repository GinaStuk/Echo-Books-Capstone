@model List<Capstone.Web.Models.BookModel>
@{
    ViewBag.Title = "New Author List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Newly Added Authors</h2>

@for (int i = 0; i < Model.Count; i++)
{



    <div class="blockForList">

        <div class="searchResultListItem">

            <h3>@Model[i].Author</h3>


        </div>
    </div>


}