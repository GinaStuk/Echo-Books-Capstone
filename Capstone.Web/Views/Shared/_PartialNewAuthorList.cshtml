@model List<String>

@{
    var authorLimit = Math.Min(10, Model.Count);
}

<div id="newAuthorList">
    <h2>Newly Added Authors</h2>

    @for (int i = 0; i < authorLimit; i++)
    {



        <div class="blockForList">

            <div class="searchResultListItem">



                <h3>@Html.ActionLink(Model[i], "SearchResults", "Book", new { searchType = "Author", SearchValue = Model[i] }, null)</h3>
            </div>
        </div>


    }
</div>
<button class="btn-warning">@Html.ActionLink("See All...", "NewAuthorList", "Home")</button>
